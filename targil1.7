function MobilePhone(model, size, price, number){
  	this.model = model;
    this.size = size;
    this.price = price;
    this.number = number;
    this.getNumber = function(){return this.number};
  	this.setNumber = function(num){this.number = num};
}

var myPhone1 = new MobilePhone("a", 3, 5, 7);
var myPhone2 = createMobilePhone("a", 3, 5, 7);

function createMobilePhone(model, size, price, number){
 	var a = {};
  a.model = model;
  a.size = size;
  a.price = price;
  a.number = number;
  a.getNumber = function(){return a.number};
  a.setNumber = function(b){a.number = b};
  return a
}
